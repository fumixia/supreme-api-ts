{
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "supreme",
    "api",
    "typescript"
  ],
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "supreme-api-ts",
  "author": "Marco Jacobs",
  "homepage": "https://github.com/fumixia/supreme-api-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fumixia/supreme-api-ts"
  },
  "bugs": {
    "url": "https://github.com/fumixia/supreme-api-ts/issues"
  },
  "module": "dist/supreme-api-ts.esm.js",
  "size-limit": [
    {
      "path": "dist/supreme-api-ts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/supreme-api-ts.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/request-promise": "^4.1.48",
    "husky": "^7.0.4",
    "request": "^2.88.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "request-promise": "^4.2.6"
  }
}
